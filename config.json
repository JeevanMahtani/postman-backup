{
  "api_url": "https://api.getpostman.com/",
  "api_keys": ["<API_KEY"],
  "path": {
    "collections": "jsonFiles/collections",
    "environments": "jsonFiles/environments"
  },
  "time_between_requests": 3000,
  "use_date_subfolder": false,
  "use_id_subfolder": false,
  "debug": {
    "appenders": {
      "debug": {
        "type": "file",
        "filename": "debug.log"
      }
    },
    "categories": {
      "default": {
        "appenders": ["debug"],
        "level": "info"
      }
    }
  }
}
